{
    "name": "Alhambra-II",
    "description": "Alhambra II FPGA board with 3 Axis SPI",
    "board": "Alhambra-II",
    "toolchain": "icestorm",
    "family": "ice40",
    "type": "hx4k",
    "package": "tq144",
    "clock": {
        "speed": "12000000",
        "pin": "49"
    },
    "transport": "UDP",
    "ip": "192.168.10.28",
    "axis": 3,
    "interface": [
        {
            "type": "spi",
            "pins": {
                "MOSI": "20",
                "MISO": "19",
                "SCK": "22",
                "SEL": "21"
            }
        }
    ],
    "blink": {
        "pin": "37"
    },
    "error": {
        "invert": true,
        "pin": "38"
    },
    "enable": {
        "pin": "39"
    },
    "plugins": [
        {
            "pullup": true,
            "pin": "63",
            "name": "DIN0",
            "type": "din_bit"
        },
        {
            "pullup": true,
            "pin": "64",
            "name": "DIN1",
            "type": "din_bit"
        },
        {
            "pin": "41",
            "name": "DOUT0",
            "type": "dout_bit"
        },
        {
            "pin": "42",
            "name": "DOUT1",
            "type": "dout_bit"
        },
        {
            "pin": "43",
            "name": "DOUT2",
            "type": "dout_bit"
        },
        {
            "pin": "44",
            "name": "DOUT3",
            "type": "dout_bit"
        },
        {
            "type": "vin_counter",
            "pullup": true,
            "pins": {
                "up": "34",
                "down": "33"
            },
            "name": "VIN0"
        },
        {
            "type": "vout_pwm",
            "pins": {
                "pwm": "45"
            },
            "name": "VOUT0"
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": 800,
            "pins": {
                "step": "1",
                "dir": "2"
            },
            "name": "JOINT0"
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": 800,
            "pins": {
                "step": "3",
                "dir": "4"
            },
            "name": "JOINT1"
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": 800,
            "pins": {
                "step": "7",
                "dir": "8"
            },
            "name": "JOINT2"
        }
    ]
}
