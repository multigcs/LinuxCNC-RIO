{
    "name": "Olimex-ICE40HX8K-EVB_BOB",
    "description": "Olimex-ICE40HX8K-EVB with cheap 5axis-BOB",
    "toolchain": "icestorm",
    "family": "ice40",
    "type": "hx8k",
    "package": "ct256",
    "clock": {
        "speed": "100000000",
        "pin": "J3"
    },
    "interface": [
        {
            "type": "spi",
            "pins": {
                "MOSI": "P12",
                "MISO": "P11",
                "SCK": "R11",
                "SEL": "L11"
            }
        }
    ],
    "error": {
        "pin": "R16"
    },
    "enable": {
        "pin": "F5"
    },
    "dout": [
        {
            "pin": "F4"
        },
        {
            "pin": "D2"
        },
        {
            "pin": "G5"
        },
        {
            "pin": "D1"
        }
    ],
    "din": [
        {
            "pullup": true,
            "pin": "H6"
        },
        {
            "pullup": true,
            "pin": "F2"
        },
        {
            "pullup": true,
            "pin": "H3"
        },
        {
            "pullup": true,
            "pin": "F3"
        },
        {
            "pullup": true,
            "pin": "B1"
        }
    ],
    "vout": [
        {
            "type": "pwm",
            "pin": "J2",
            "dir": "J14"
        },
        {
            "type": "pwm",
            "min": "-10",
            "max": "+10",
            "pin": "H5",
            "dir": "J12"
        }
    ],
    "vin": [
        {
            "type": "quadencoder",
            "pullup": true,
            "pins": {
                "a": "E2",
                "b": "G3"
            }
        }
    ],
    "joints": [
        {
            "type": "stepper",
            "cl": false,
            "pins": {
                "step": "H1",
                "dir": "G1"
            }
        },
        {
            "type": "stepper",
            "cl": false,
            "pins": {
                "step": "J5",
                "dir": "H2"
            }
        },
        {
            "type": "stepper",
            "cl": false,
            "pins": {
                "step": "J4",
                "dir": "G2"
            }
        },
        {
            "type": "stepper",
            "cl": false,
            "enc_scale": "2",
            "pins": {
                "step": "H4",
                "dir": "F1"
            }
        },
        {
            "type": "stepper",
            "cl": false,
            "pins": {
                "step": "C2",
                "dir": "C1"
            }
        }
    ]
}
