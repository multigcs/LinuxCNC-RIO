{
    "name": "Tangoboard",
    "description": "Tangoboard with TangNano9K over SPI",
    "boardcfg": "TangNano9K",
    "images": {
        "board": "tango.png",
        "pinout": "tangnano9k-board-pinout.png",
        "fpgaboard": "TangNano9K-pinout.png"
    },
    "transport": "UDP",
    "_gui": "qtdragon",
    "interface": [
        {
            "type": "w5500",
            "mac": "AA:AF:FA:CC:E3:1C",
            "ip": "192.168.10.194",
            "port": "2390",
            "pins": {
                "MOSI": "48",
                "MISO": "49",
                "SCK": "31",
                "SEL": "32"
            }
        }
    ],
    "enable": {
        "invert": true,
        "pin": "73"
    },
    "plugins": [

        {


            "type": "dout_bit",
            "name": "spindle-enable",
            "net": "spindle.0.on",
            "invert": true,
            "pin": "38"
        },
        {
            "type": "dout_bit",
            "invert": true,
            "pin": "36"
        },
        {
            "type": "dout_bit",
            "invert": true,
            "pin": "39"
        },
        {
            "type": "dout_bit",
            "invert": true,
            "pin": "25"
        },
        {
            "type": "din_bit",
            "name": "home-x",
            "net": "joint.0.home-sw-in",
            "invert": true,
            "pullup": true,
            "pin": "26"
        },
        {
            "type": "din_bit",
            "name": "home-y",
            "net": "joint.1.home-sw-in",
            "invert": true,
            "pullup": true,
            "pin": "27"
        },
        {
            "type": "din_bit",
            "name": "home-z",
            "net": "joint.2.home-sw-in",
            "invert": true,
            "pullup": true,
            "pin": "28"
        },
        {
            "type": "din_bit",
            "name": "tool-probe",
            "net": "motion.probe-input",
            "invert": true,
            "pullup": true,
            "pin": "29"
        },
        {
            "type": "din_bit",
            "invert": true,
            "pullup": true,
            "pin": "53"
        },
        {
            "type": "din_bit",
            "invert": true,
            "pullup": true,
            "pin": "54"
        },
        {
            "type": "din_bit",
            "invert": true,
            "pullup": true,
            "pin": "55"
        },
        {
            "type": "din_bit",
            "invert": true,
            "pullup": true,
            "pin": "56"
        },
        {
            "name": "spindle-speed",
            "net": "spindle.0.speed-out",
            "min": 0,
            "max": 10000,
            "invert": true,
            "type": "vout_pwm",
            "pins": {
                "pwm": "76"
            }
        },
        {
            "type": "vout_pwm",
            "invert": true,
            "pins": {
                "pwm": "75",
                "dir": "74"
            }
        },
        {
            "type": "vout_pwm",
            "frequency": "1500",
            "invert": true,
            "pins": {
                "pwm": "37"
            }
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": -800,
            "pins": {
                "step": "63",
                "dir": "86"
            }
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": -800,
            "pins": {
                "step": "85",
                "dir": "84"
            }
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "scale": -1600,
            "pins": {
                "step": "83",
                "dir": "82"
            }
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "pins": {
                "step": "81",
                "dir": "80"
            }
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "pins": {
                "step": "79",
                "dir": "77"
            }
        }
    ]
}
